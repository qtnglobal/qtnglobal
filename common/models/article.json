      "type": "string",
      "required": true
    },
    "content": {
      "type": "string",
      "required": true
    },
    "url": {
      "type": "string"
    },
    "flag": {
      "type": "string"
    },
    "tag": {
      "type": "string"
    },
    "image": {
      "type": "string"
    },
    "audio": {
      "type": "string"
    },
    "video": {
      "type": "string"
    },
    "files": {
      "type": "string"
    }
